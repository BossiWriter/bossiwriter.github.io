{
  "ci": {
    "collect": {
      "numberOfRuns": 1,
      "url": [
        "https://bossiwriter.github.io/",
        "https://bossiwriter.github.io/docs/intro",
        "https://bossiwriter.github.io/docs/about-me/about-me",
        "https://bossiwriter.github.io/docs/case-studies/behind-the-docs",
        "https://bossiwriter.github.io/docs/github-api-doc/guides/quick-start"
      ],
      "settings": {
        "preset": "mobile",
        "chromeFlags": ["--no-sandbox", "--disable-dev-shm-usage"]
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": "off",
        "categories:accessibility": "off",
        "categories:best-practices": "off",
        "categories:seo": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
