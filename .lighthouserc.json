{
  "ci": {
    "collect": {
      "url": ["https://bossiwriter.github.io/"],
      "numberOfRuns": 1,
      "settings": {
        "preset": "perf",
        "emulatedFormFactor": "mobile",
        "throttlingMethod": "simulate",
        "chromeFlags": ["--no-sandbox", "--disable-dev-shm-usage"]
      }
    },
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions": {}
    },
    "upload": {
      "target": "filesystem"
    }
  }
}
