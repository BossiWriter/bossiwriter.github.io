{
  "ci": {
    "collect": {
      "url": ["https://bossiwriter.github.io/"],
      "numberOfRuns": 1,
      "outputDir": ".lighthouseci",
      "settings": {
        "preset": "perf",
        "formFactor": "mobile",
        "screenEmulation": {
          "mobile": true,
          "width": 360,
          "height": 640,
          "deviceScaleFactor": 2,
          "disabled": false
        },
        "throttlingMethod": "simulate"
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": "warn"
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": ".lighthouseci"
    }
  }
}
